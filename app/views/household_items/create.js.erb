// Add item to the table.
var $tbody        = $("#household_item_table tbody")
var newRecordHTML = "<%= j render 'household_items/table_row', household_item: @household_item %>"
$tbody.prepend(newRecordHTML)

// Update flash.
$('#flash_message').html("<%= j render 'layouts/flash_message' %>")

// Update the chart and total volume in moving stats.
$('#moving_stats').html("<%= j render 'household_items/moving_stats', moving: @moving, household_items: @household_items, data: @data %>")
